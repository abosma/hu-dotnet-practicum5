<Page x:Class="WPFGUI.RegisterPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPFGUI"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="RegisterPage">

    <Grid>
        <TextBox Name="UsernameTextbox" HorizontalAlignment="Left" Height="24" Margin="81,50,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label Name="PasswordLabel" Content="Label" HorizontalAlignment="Left" Margin="81,98,0,0" VerticalAlignment="Top" Visibility="Hidden"/>
        <Button Click="Register" Content="Register" HorizontalAlignment="Left" Margin="153,144,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Click="Return" Content="Return" HorizontalAlignment="Left" Margin="56,144,0,0" VerticalAlignment="Top" Width="75"/>
        <Label Content="Username" HorizontalAlignment="Left" Margin="9,45,0,0" VerticalAlignment="Top"/>

    </Grid>
</Page>
